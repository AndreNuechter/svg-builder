{
    "extends": "airbnb-base",
    "rules": {
        "indent": ["error", 4],
        "comma-dangle": ["error", "never"],
        "max-len": ["error", {
            "code": 100,
            "ignoreComments": true,
            "ignoreRegExpLiterals": true
        }],
        "no-use-before-define": ["error", {
            "functions": false
        }],
        "eol-last": ["error", "never"],
        "no-param-reassign": ["error", { "props": false }]
    }
}